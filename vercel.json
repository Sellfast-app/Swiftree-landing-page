{
    "builds": [
      {
        "src": "swiftree/apps/landing/package.json",
        "use": "@vercel/static-build",
        "config": { "distDir": "dist" }
      },
      {
        "src": "swiftree/apps/storefront/package.json",
        "use": "@vercel/next"
      }
    ],
    "routes": [
      { "src": "/storefront/(.*)", "dest": "/storefront/$1" },
      { "src": "/(.*)", "dest": "/index.html" }
    ]
  }
  