{
    "projects": [
      {
        "rootDirectory": "swiftree/apps/landing",
        "outputDirectory": "dist",
        "buildCommand": "npm install && npm run build",
        "rewrites": [
          { "source": "/", "destination": "/index.html" }
        ]
      },
      {
        "rootDirectory": "swiftree/apps/storefront",
        "buildCommand": "npm install && npm run build",
        "rewrites": [
          { "source": "/storefront/:path*", "destination": "/:path*" }
        ]
      }
    ]
  }
  